{
  "name": "EleventyOne",
  "version": "3.0.0",
  "description": "A project bootstrap for Eleventy sites. With pipelines for PostCSS, JavaScript, and serverless functions",
  "author": "Phil Hawksworth",
  "license": "MIT",
  "scripts": {
    "sass": "sass --style=compressed src/scss/style.scss src/site/css/style.css",
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "seed": "cross-env ELEVENTY_ENV=seed eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philhawksworth/eleventyone"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "postcss": "^7.0.36",
    "postcss-color-mix": "^1.1.0",
    "postcss-comment": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-simple-vars": "^5.0.2",
    "precss": "^4.0.0",
    "sass": "^1.32.2",
    "uglify-js": "^3.12.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "cross-env": "7.0.2",
    "npm-run-all": "^4.1.5"
  }
}
